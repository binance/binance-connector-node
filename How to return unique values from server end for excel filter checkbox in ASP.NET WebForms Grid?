  0% { opacity: 1; transform: translate(0px, 0px) scale(1); }
  100% { opacity: 0; transform: translate(0px, 0px) scale(3); }
}

@keyframes ProgressZoomOut { 
  0% { transform: translate(0px, 0px) scale(1); }
  100% { transform: translate(0px, 0px) scale(0); }
}


.e-ripple, .e-ripple-style { overflow: hidden; position: relative; transform: translate3d(0px, 0px, 0px); }

.e-ripple-element, .e-ripple-style::after { background-color: rgba(0, 0, 0, 0.1); border-radius: 0px; overflow: hidden; pointer-events: none; position: absolute; transform: scale(0); transition: opacity, transform cubic-bezier(0, 0.1, 0.2, 1); }

.e-ripple-style::after { background: rgba(0, 0, 0, 0.3); content: ""; display: block; height: 50%; left: 25%; opacity: 0; top: 25%; transform: scale(3); transition: transform 0.5s, opacity 0.5s; width: 50%; }

.e-ripple-style:active::after { opacity: 1; transform: scale(0); transition: all; }

